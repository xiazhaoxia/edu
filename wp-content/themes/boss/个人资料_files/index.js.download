		var left = 2;
		if (money % 100 == 0) {
			left = 0;
		} else if (money % 10 == 0) {
			left = 1;
		}
		return money.toFixed(left).toString().replace(/(\d)(?=(\d{3})+$)/g, "$1,");
	}

