/*zonghe   page*/
.zonghe-wrap{
	width:100%;
}
.zonghe-top{
	padding:10px 10px 10px 10px;
	margin-bottom:15px;
}
.top-img{
	width:100%;
}
.zonghe-top h3{
	font-size: 16px !important;
}
.zonghe-top{
	font-size: 12px;
	color:#8f8f8f;
}
.zonghe-content{
	padding-left: 13px;
    padding-right: 13px;
    margin-bottom: 0;
	width:100%;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
	position:relative;
	z-index:999;
	border-top:1px solid #ecedef;
}
.zonghe-content ul {
	width:100%;
	overflow:hidden;
}
.zonghe-content ul li{
	list-style:none;
	position:relative;
}
.zonghe-content li a{
	display: block;
    padding: 10px 0;
    overflow: hidden;
}
.zonghe-title{
	height:44px;
	overflow:hidden;
	text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    display: -webkit-box;
    -webkit-line-clamp: 2;
	line-clamp: 2;
	line-height: 22px;
    font-size: 16px;
    color: #2b2b2b;
	display:block;
}
.zonghe-info{
	position: absolute;
    left: 128px;
    bottom: 12px;
	display: block;
    font-size: 14px;
    color: #a4a4a4;
}
.zonghe-right{
	position: relative;
    float: left;
    width: 114px;
    height: 75px;
    margin-right: 12px;
    overflow: hidden;
}
.zonghe-right img{
	width: 100%;
    min-height: 100%;
    vertical-align: top;
    object-fit: cover;
}

/*channels*/
.wll-sub-nav{
	padding-bottom:60px;
}
.wll-sub-nav ul{
	display: flex;
	-webkit-display: flex;
	flex-wrap:wrap;
}
.wll-sub-nav ul li{
    width: 33.33% !important;
	background:#fff;
	position:relative;
	margin-left:0;
	background:#fff;
}
.wll-sub-nav ul li:after{	
	content:'';
	position:absolute;
	top:0;
	right:0;
	height:100%;
	width:1px;
	background: #ccc;
	transform:scaleX(0.5);
	-webkit-transform:scaleX(0.5);
	-moz-transform:scaleX(0.5);
}
.wll-sub-nav ul li a{
	width:100%;
	display:block;
	position:relative;	
	height:auto;
	padding:20px 0 10px 0;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	-box-sizing:border-box;	
}
.wll-sub-nav ul li a:link{
	background:#fff;
}
.wll-sub-nav ul li a:after{
	content:'';
	position:absolute;
	bottom:0;
	left:0;
	height:1px;
	width:100%;
	background:  #ccc;
	transform:scaleY(0.5);
	-webkit-transform:scaleY(0.5);
	-moz-transform:scaleY(0.5);
}
.wll-sub-nav ul li:nth-child(3):after,
.wll-sub-nav ul li:nth-child(6):after,
.wll-sub-nav ul li:nth-child(9):after
{
	content:'';
	width:0;
}
.wll-sub-nav ul li a:before{
	content:'';
}
.wll-sub-nav ul li a span{
	width: 80px;
    height: 80px;
    display: block;
    margin: 0 auto 10px;
}
.wll-sub-nav ul li a span img{
	width:100%;
	height:100%;
	border:0;
}
.hll-header-top .selected{
	position:relative;
	
}
.hll-header-top .selected:after{
	content:'';
	width:100%;
	height:2px;
	background:#fff;
	position:absolute;
	bottom:6px;
	left:0;
}
